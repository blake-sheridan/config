[alias]
a = add
ap = add --patch
b = branch
bl = branch --list
blr = branch --list --remote
c = checkout
ca = commit --amend
cm = commit --message
co = checkout
cop = checkout --patch
com = checkout master
d = diff
ds = diff --staged
f = fetch
l = log
lf = log --follow
lp = log -p
p = pull
pr = pull --rebase
ra = rebase --abort
rc = rebase --continue
rh = reset HEAD --
rs = rebase --skip
ri = rebase --interactive
s = status
sh = show
st = status --untracked-files=no
su = submodule update
sui = submodule update --init
suir = submodule update --init --recursive

