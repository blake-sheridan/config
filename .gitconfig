[alias]
a = add
ap = add --patch
b = branch
bl = branch --list
blr = branch --list --remote
c = checkout
cm = commit --message
co = checkout
com = checkout master
cp = checkout --patch
d = diff
ds = diff --staged
f = fetch
l = log
lf = log --follow
lp = log -p
p = pull
pr = pull --rebase
psu = !git push --set-upstream origin $(git branch | grep \\* | cut -d ' ' -f2)
ra = rebase --abort
rc = rebase --continue
rh = reset HEAD --
rs = rebase --skip
ri = rebase --interactive
s = status
sh = show
st = status --untracked-files=no
su = submodule update
sui = submodule update --init
suir = submodule update --init --recursive

[clean]
requireForce = false

[color]
status = always

[core]
editor = emacs

[log]
abbrevCommit = true

[push]
default = simple

[user]
name = Blake Sheridan
